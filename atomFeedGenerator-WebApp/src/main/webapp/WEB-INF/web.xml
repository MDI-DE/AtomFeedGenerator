<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>atomFeed</display-name>
	<servlet>
		<description>
		</description>
		<display-name>OpenSearchImpl</display-name>
		<servlet-name>OpenSearchImpl</servlet-name>
		<servlet-class>mdide.openSearch.OpenSearchImpl</servlet-class>
	</servlet>
	<servlet>
    	<servlet-name>Initialization</servlet-name>
    	<servlet-class>mdide.atomFeedGen.Initialization</servlet-class>
    	<load-on-startup>1</load-on-startup>
 	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>Client</display-name>
		<servlet-name>Client</servlet-name>
		<servlet-class>mdide.openSearch.Client</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>StartFeedGen</display-name>
		<servlet-name>StartFeedGen</servlet-name>
		<servlet-class>mdide.atomFeedGen.StartFeedGen</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>DocumentsServlet</display-name>
		<servlet-name>DocumentsServlet</servlet-name>
		<servlet-class>mdide.atomFeedGen.DocumentsServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>RestClient</display-name>
		<servlet-name>RestClient</servlet-name>
		<servlet-class>mdide.openSearch.RestClient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OpenSearchImpl</servlet-name>
		<url-pattern>/OpenSearchService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Client</servlet-name>
		<url-pattern>/Client</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StartFeedGen</servlet-name>
		<url-pattern>/StartFeedGen</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DocumentsServlet</servlet-name>
		<url-pattern>/getFeedDoc</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RestClient</servlet-name>
		<url-pattern>/api/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>